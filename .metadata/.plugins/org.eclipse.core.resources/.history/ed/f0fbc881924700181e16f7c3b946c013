
public class PracticeWhile {
	static int max=20;
	public static void main(String []args)
	{
		float sum = 0;
		int index = 1;
		float factorial = 1;
		while(index <= max)
		{
			
			int index2 = 1;
			while(index2<=index)
			{
				factorial /= index2;
				++index2;
			}
			sum += factorial;
			++index;
		}
		System.out.println(sum);
	}
}
